   "source": [
    "def maxSubArraySum(arr):\n",
    "        maxsum=cursum = arr[0]\n",
    "        for x in range(1,len(arr)):\n",
    "            cursum=max(cursum+arr[x],arr[x]) \n",
    "            if cursum>maxsum:\n",
    "                maxsum=cursum       \n",
    "        return maxsum\n",
    "    \n",
    "x=maxSubArraySum([1 ,2 ,3 ,-2, 5])\n",
    "print(x)"
   ]
